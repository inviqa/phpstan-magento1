{
    "name": "vianetz/phpstan-magento1",
    "description": "Extension for PHPStan to allow analysis of Magento 1 code.",
    "type": "library",
    "require": {
        "phpstan/phpstan": "^0.11|^0.12",
        "php": ">= 7.2"
    },
    "replace": {
        "inviqa/phpstan-magento1": "0.1.5"
    },
    "autoload": {
        "psr-4": {
            "PHPStanMagento1\\": "src/"
        }
    },
    "scripts": {
        "test-quality": [
            "phpstan analyse"
        ],
        "test": [
            "@test-quality"
        ]
    },
    "license": "MIT"
}
